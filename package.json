{
  "name": "vt",
  "version": "1.0.0",
  "description": "Fastify + Postgres app",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "npm run css:build && lsof -ti:3000 | xargs kill -9 2>/dev/null; node src/server.js",
    "dev": "lsof -ti:3000 | xargs kill -9 2>/dev/null; concurrently -k \"npm run css:watch\" \"node --watch src/server.js\"",
    "css:build": "npx @tailwindcss/cli -i ./src/styles/admin.css -o ./public/admin.css --minify",
    "css:watch": "npx @tailwindcss/cli -i ./src/styles/admin.css -o ./public/admin.css --watch",
    "logs:clear": "rm -f logs/*.log",
    "migrate": "node src/migrations/runner.js",
    "seed": "node src/seed/runner.js",
    "seed:user": "node src/seed/runner.js user",
    "seed:exercises": "node src/seed/runner.js exercises",
    "seed:voice-profile": "node src/seed/runner.js voice-profile",
    "db:reset": "node src/db-reset.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.971.0",
    "@aws-sdk/s3-request-presigner": "^3.971.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/multipart": "^9.4.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.1.0",
    "argon2": "^0.44.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "concurrently": "^9.2.1",
    "pino-pretty": "^13.1.3",
    "tailwindcss": "^4.1.18"
  }
}
